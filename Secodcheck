#guess a nuler
import random

 
guessnumber = random.randint(0, 100)
print(guessnumber)
found = False

while not found:
    number = int(input('number given '))
    if guessnumber > number:
        print('too high')
        found = False
    elif guessnumber < number:
        print('loow')
        found = False
    else:
        print('Bingo')
        found = True
